{
  "name": "@armix/server-schema",
  "version": "0.1.5",
  "author": "Peter Boyer <@ptboyer>",
  "description": "Generate TypeDefs, Resolvers, and Tables for your GraphQL/Knex backends.",
  "repository": "https://github.com/armix-io/armix-server",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "yarn build:ts",
    "build:ts": "tsc --skipLibCheck",
    "watch:ts": "yarn build:ts -w",
    "watch:yalc": "yalc push && nodemon --delay 2500ms --exec \"yalc push --changed\"",
    "dev": "concurrently -k -p \"[{name}]\" -n \"ts,yalc\" -c \"cyan.bold,magenta.bold\" \"yarn watch:ts\" \"yarn watch:yalc\"",
    "docs": "jsdoc src/*.js -d ./docs/",
    "docs:open": "open ./docs/index.html",
    "push": "../../publish.sh",
    "test": "ts-node node_modules/tape/bin/tape ./src/graph/field-types.test.ts ./src/core/index.test.ts"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.159",
    "@types/tape": "^4.13.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "apollo-server": "^2.19.0",
    "apollo-server-testing": "^2.19.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.6.0",
    "graphql": "^14.0.0",
    "jsdoc": "^3.6.4",
    "knex": "^0.21.12",
    "knex-postgis": "^0.12.0",
    "nodemon": "^2.0.4",
    "pg": "^8.4.2",
    "prettier": "^2.0.5",
    "tape": "^5.0.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
