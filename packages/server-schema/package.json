{
  "name": "@armix/server-schema",
  "version": "0.2.0",
  "author": "Peter Boyer <@ptboyer>",
  "description": "Generate TypeDefs, Resolvers, and Tables for your GraphQL/Knex backends.",
  "repository": "https://github.com/armix-io/armix-server",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "yarn build:ts",
    "build:ts": "tsc",
    "watch:ts": "yarn build:ts -w",
    "watch:yalc": "yalc push && nodemon --delay 2500ms --exec \"yalc push --changed\"",
    "dev": "concurrently -k -p \"[{name}]\" -n \"ts,yalc\" -c \"cyan.bold,magenta.bold\" \"yarn watch:ts\" \"yarn watch:yalc\"",
    "docs": "jsdoc src/*.js -d ./docs/",
    "docs:open": "open ./docs/index.html",
    "push": "../../publish.sh",
    "test": "jest"
  },
  "dependencies": {
    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.pick": "^4.4.6",
    "lodash": "^4.17.15",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.159",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "apollo-server": "^2.19.0",
    "apollo-server-testing": "^2.19.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.6.0",
    "graphql": "^14.0.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.4",
    "knex": "^0.21.12",
    "knex-postgis": "^0.12.0",
    "nodemon": "^2.0.4",
    "pg": "^8.4.2",
    "prettier": "^2.0.5",
    "sqlite3": "^5.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
