{
  "name": "@armix/server-schema",
  "version": "0.2.1",
  "author": "Peter Boyer <@ptboyer>",
  "description": "Generate TypeDefs and Resolvers for your GraphQL backends.",
  "repository": "https://github.com/armix-io/armix-server",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "yarn build:ts",
    "build:ts": "tsc",
    "watch:ts": "yarn build:ts -w",
    "watch:yalc": "cp ./package.json lib/package.json && nodemon --delay 1500ms --exec \"cd ./lib && yalc push --replace\"",
    "dev": "concurrently -k -p \"[{name}]\" -n \"ts,yalc\" -c \"cyan.bold,magenta.bold\" \"yarn watch:ts\" \"yarn watch:yalc\"",
    "deploy": "../../publish.sh",
    "test": "jest"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0",
    "lodash.identity": "^3.0.0",
    "lodash.omit": "^4.5.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.identity": "^3.0.6",
    "@types/lodash.omit": "^4.5.6",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "apollo-server": "^2.19.0",
    "apollo-server-testing": "^2.19.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.6.0",
    "graphql": "^14.0.0",
    "jest": "^26.6.3",
    "knex": "^0.21.12",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
